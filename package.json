{
  "name": "@permaweb/aoconnect",
  "version": "0.0.55",
  "repository": {
    "type": "git",
    "url": "https://github.com/scottroot/aoconnect.git"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "browser": "./dist/browser.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./browser": "./dist/browser.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": "./dist/browser.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "browser": [
        "./dist/index.d.ts"
      ],
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build-esm": "tsc --project esm.tsconfig.json",
    "build-cjs": "tsc --project cjs.tsconfig.json",
    "builder": "npm-run-all clean build-esm build-cjs",
    "build:cjs": "webpack --config webpack.common.cjs --env cjs",
    "build:browser": "webpack --config webpack.common.cjs --env browser",
    "build": "npm-run-all clean build:esm build:cjs build:browser",
    "ai-build:esm": "webpack --config webpack.esm.js",
    "ai-build:cjs": "webpack --config webpack.cjs.js",
    "ai-build:browser": "webpack --config webpack.browser.js",
    "ai-build:src": "webpack --config webpack.common.cjs",
    "ai-build": "clear && npm-run-all clean ai-build:src",
    "ai-build2": "npm run ai-build:esm && npm run ai-build:cjs && npm run ai-build:browser",
    "ai-lint": "eslint 'src/**/*.{ts,js}'",
    "ai-lint:fix": "eslint 'src/**/*.{ts,js}' --fix",
    "ai-format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "build4": "npm run clean && npm run build:src:esm && npm run build:src:cjs && npm run build:src:types",
    "build:src:cjs": "tsc --outDir dist/cjs --module CommonJS",
    "build:src:esm": "tsc --outDir dist/esm --module ESNext",
    "build:src:types": "tsc --skipLibCheck --declaration --allowJs --emitDeclarationOnly --sourceMap --outDir dist/types",
    "build2": "npm run clean && npm run build2:src:esm && npm run build2:src:cjs && npm run build2:src:types",
    "build2:src:cjs": "tsc --project cjs.tsconfig.json",
    "build2:src:esm": "tsc --project esm.tsconfig.json",
    "build2:src:types": "tsc --project types.tsconfig.json",
    "clean": "rm -rf dist && rm -rf build",
    "test3": "node tests/connect.test.js",
    "build3": "npm-run-all clean build2:src:* test3",
    "test": "node ./tests/connect.test.js",
    "pretest:integration": "npm run build && npm link",
    "test:integration": "cd ./test/e2e && npm test",
    "lint:check": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "esbuild": "npm run clean && clear && node esbuild.js && npm run test"
  },
  "dependencies": {
    "@eslint/migrate-config": "^1.0.1",
    "@permaweb/ao-scheduler-utils": "~0.0.16",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "debug": "4.3.5",
    "esm": "^3.2.25",
    "mnemonist": "^0.39.8",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "warp-arbundles": "^1.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^18.19.34",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "arconnect": "^0.4.2",
    "arweave": "^1.14.4",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "esbuild": "^0.21.5",
    "esbuild-plugin-tsc": "^0.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.4.0",
    "npm-run-all": "^4.1.5",
    "schema-utils": "^4.2.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "webpack": "=5.76.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "=5.8.0",
    "webpack-node-externals": "=3.0.0"
  },
  "engines": {
    "node": ">=18",
    "yarn": "please-use-npm"
  }
}
