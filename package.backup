{
  "name": "@permaweb/aoconnect",
  "version": "0.0.55",
  "repository": {
    "type": "git",
    "url": "https://github.com/scottroot/aoconnect.git",
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "browser": "./dist/browser.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./browser": "./dist/browser.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": "./dist/browser.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "browser": [
        "./dist/index.d.ts"
      ],
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc && node esbuild.js",
    "test": "node --test ./src",
    "pretest:integration": "npm run build && npm link",
    "test:integration": "cd ./test/e2e && npm test",
    "dev:build": "rm -rf build && concurrently \" yarn tsc -p cjs.tsconfig.json && chmod +x ./build/cjs/node/cli.js && sh ./scripts/fix-pkg.sh cjs commonjs \"  \" yarn tsc -p esm.tsconfig.json && chmod +x ./build/esm/node/cli.js && sh ./scripts/fix-pkg.sh esm module && yarn tsc-esm-fix --tsconfig esm.tsconfig.json  \" ",
    "prod:build": "yarn run dev:build && webpack",
    "lint:check": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "watch": "tsc --watch -p cjs.tsconfig.json",
    "watch:webpack": "yarn watch & webpack --watch",
    "webpack:analyze": "webpack --profile --json /tmp/wp.json && webpack-bundle-analyzer /tmp/wp.json",
    "prepack": "yarn ts-node ./scripts/embedVersion.ts",
    "postpublish": "git push --tags"
  },
  "dependencies": {
    "@permaweb/ao-scheduler-utils": "~0.0.16",
    "buffer": "^6.0.3",
    "debug": "^4.3.4",
    "mnemonist": "^0.39.8",
    "warp-arbundles": "^1.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@digitak/tsc-esm": "^3.1.4",
    "@manahippo/aptos-wallet-adapter": "^0.3.6",
    "@microsoft/api-extractor": "^7.34.4",
    "@near-js/wallet-account": "^0.0.4",
    "@privy-io/react-auth": "^1.52.6",
    "@solana/wallet-adapter-base": "^0.9.2",
    "@types/async-retry": "^1.4.3",
    "@types/base64url": "^2.0.0",
    "@types/bn.js": "^5.1.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^18.0.0",
    "@types/ramda": "^0.30.0",
    "@types/secp256k1": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "arconnect": "^0.4.2",
    "arweave": "^1.14.4",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.0.1",
    "crypto-browserify": "^3.12.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "longjohn": "^0.2.12",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.4",
    "prettier-eslint": "^16.3.0",
    "process": "^0.11.10",
    "resolve-tspaths": "^0.8.8",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.1",
    "tsc-esm-fix": "^2.20.12",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2",
    "viem": "^2.7.1"
  },
  "engines": {
    "node": ">=18",
    "yarn": "please-use-npm"
  }
}
